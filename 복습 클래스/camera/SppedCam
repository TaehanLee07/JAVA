package camera;

public class SppedCam extends Camera{

    public SppedCam() {
        this.name = "과속단속 카메라";
    }

    public void checkSpped() {
        System.out.println("속도를 측정합니다");
    }

    public void recongizeLicnesePlate(){
        // 번호인식
        System.out.println("차량번호를 인식합니다");
    }

    @Override // annotation
    public void showMainFeature() {
        System.out.println(this.name + "의 주요 기능 : 속도 측정, 번호 인식");
    }
}
